{
    "family": "my-nestjs-app",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "nestjs",
        "image": "my_webapp_image:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000,
            "protocol": "tcp"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/my-nestjs-app",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "volumes": [
      {
        "name": "nestjs-volume",
        "host": {
          "sourcePath": "/ecs/nestjs"
        }
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }
  